            TxtArea_Bloqueado.append("\n" + Proc.nuevoB.proceso + ".[" + Proc.nuevoB.tiempo + "]" + "[" + Proc.nuevoB.TBLOQ + "][" + Proc.nuevoB.prior + "]");                
            jButtonQuantum.doClick();
        }
